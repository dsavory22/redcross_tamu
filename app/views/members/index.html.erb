<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Fall dues</th>
      <th>Spring dues</th>
      <th>Shirt size</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.First_Name %></td>
        <td><%= member.Last_Name %></td>
        <td><%= member.Email %></td>
        <td><%= member.Fall_Dues %></td>
        <td><%= member.Spring_Dues %></td>
        <td><%= member.Shirt_Size %></td>
        <td><%= member.year %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
